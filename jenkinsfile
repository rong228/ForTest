pipeline {
    agent any
    stages {
        stage ("Check www.evga.com response code") {
            steps{
                sh  '''
                    TOKEN="test-token"
                    GROUP_ID="-000000000"
                    CODE=$(curl -o /dev/null -s -w %{http_code} www.evga.com)
                    
                    
                    if [ "${CODE}" != "200" ]; then
                      echo "return not 200"
					else
					  eche "retur 200"
                    fi                
                '''
            }
        }
    }
}